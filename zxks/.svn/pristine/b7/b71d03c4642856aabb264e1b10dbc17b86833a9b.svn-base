    <script type="text/javascript">
    //左边导航栏打开状态设置
    $(".mid-left .nav .mid-left-li").eq(2).addClass("open");
    $(".mid-left .nav .mid-left-li").eq(2).children('ul').slideDown();

    // 题库链接
    $(".book-sample .r-down-middle .table tbody td ").on("click","a",function(evernt) {
        var TestbookId=$(this).parent('td').siblings('td').eq(0).text();
        var url01 = "<c:url value='/QuestionBank/query3' />";
        //alert(TestbookId);
        
        $.ajax( {
            type : "POST",   
            url : url01, 
            data : 
            {
              'TestbookId' : TestbookId,
             },  
            dataType: "json",   
            success : function(data) 
            {
                //var data = eval(data);
                
                var tbody = $('#tbody001');
                tbody.empty();
                for (var i=0; i<data.questionbankList.length; i++)
                {
                    var tr = $("<tr></tr>");
                    $("<td></td>").html(data.questionbankList[i].questionbankId).appendTo(tr);
                    $("<td></td>").html(data.questionbankList[i].paperType).appendTo(tr);
                    $("<td></td>").html(data.questionbankList[i].testbookId).appendTo(tr);
                    $("<td></td>").html(data.questionbankList[i].writer).appendTo(tr);
                    $("<td></td>").html(data.questionbankList[i].inTime).appendTo(tr);
                    $("<td></td>").html(data.questionbankList[i].status).appendTo(tr);
                    var atip = "<a href='<%=classpath%>/QuestionBank/passOrNot2?type=Yes&questionbankId="+data.questionbankList[i].questionbankId+"'>通过</a>/<a href='<%=classpath%>/QuestionBank/passOrNot2?type=No&questionbankId="+data.questionbankList[i].questionbankId+"'>作废</a>"
                    $("<td></td>").html(atip).appendTo(tr);
                    tbody.append(tr); 
                }                      
            },   
            error :function(){   
                alert("网络连接出错！");   
            }   
        });
    });

    //题库连接
    function query22 () {
        
    };
    </script>
