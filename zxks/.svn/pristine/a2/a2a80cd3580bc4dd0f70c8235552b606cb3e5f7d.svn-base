a {
    text-decoration: none;
    outline: none;
}
body {
    font-family: 'Open Sans', sans-serif;
    color:#444;
    font-size: 13px;
    line-height: 25px;
    -webkit-font-smoothing: antialiased;
}
.container-fluid {
    padding: 0px;
    margin: 0px;
}


/* 澶撮儴 */

.header {
    height: 58px;
    background-color: #16CBE6;
}

.header h1 a {
    color: #fff;
    font-family: 'kaiti', sans-serif;
    font-size: 22.2px;
    text-decoration: none;
}

.header h1 {
    margin-top: 6px;
}

.header .navbar {
    margin-bottom: 0px;
}

.header .navbar .nav li {
    text-align: center;
}

.header .navbar .nav .dropdown:nth-of-type(1) .dropdown-menu {
    min-width: 125px;
}

.header .navbar .nav .dropdown:nth-of-type(2) .dropdown-menu {
    min-width: 226px;
}


/* .header .navbar .nav {
    padding-top: 7px;
} */

.header .navbar .nav .label {
    font-size: 12px;
    padding: 4px 8px;
    margin: 0px 2px;
    border-radius: 15px;
}

.header .navbar .dropdown-a {
    color: white;
    font-size: 15px;
    font-weight: bold;
    padding-top: 22px;
}

.header .navbar .dropdown-a:focus,
.header .navbar .dropdown-a:hover,
.header .navbar .dropdown-a:active {
    background-color: #1BB6E3;
}


/* 涓棿閮ㄥ垎 */


/* 涓棿閮ㄥ垎-宸﹁竟瀵艰埅鏍�*/

.middle {
    margin-top: 40px;
}

.middle .mid-left .nav {
    width: 100%;
}

.middle .mid-left .nav li {
    width: 100%;
    border-bottom: 1px dashed #eee;
}

.middle .mid-left .navbar-default {
    background-color: white;
}

.middle .mid-left .navbar-brand i {
    margin-right: 7px;
}

.middle .mid-left .nav li ul {
    margin-top: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    display: none;
    float: left;
    width: 100%;
    padding: 5px 0px;
    font-size: 14px;
    text-align: left;
    list-style: none;
    background-color: #fff;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border: 1px solid #ccc;
    border: 1px solid rgba(0, 0, 0, .15);
    border-radius: 4px;
    -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
    box-shadow: 0 6px 12px rgba(0, 0, 0, .175);
}

.middle .mid-left .nav li a i {
    margin-right: 7px;
}

.middle .mid-left .nav li a b {
    margin-left: 10px;
}

.middle .mid-left .nav li ul li a {
    display: block;
    padding: 3px 20px 3px 29px;
    clear: both;
    font-weight: 400;
    line-height: 1.42857143;
    color: #333;
    white-space: nowrap;
    text-decoration: none;
}

.middle .mid-left .nav li ul li a:hover,
.middle .mid-left .nav li ul li a:active {
    background-color: #E7E7E7;
}


/* 涓棿閮ㄩ棬 鍙宠竟 */

.mid-right-up h2 {
    font-size: 35px;
